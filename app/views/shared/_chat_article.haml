- @group_message.each do |message|
  .main-contents__chat-list__text-box.chat{data: {id: message.id, group_id: message.group_id}}
    %h4=message.user.name
    .post_time=message.created_at.strftime('%Y年%m月%d日')
    .post_time=message.created_at.strftime('%H:%M:%S')
    %p=message.body
    =image_tag(message.image)
